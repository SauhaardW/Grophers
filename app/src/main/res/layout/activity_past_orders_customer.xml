<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PastOrdersCustomer">

    <ImageView
        android:id="@+id/imageViewCustomerCartLogo"
        android:layout_width="49dp"
        android:layout_height="59dp"
        android:layout_marginTop="9dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/customer_logo" />

    <ImageView
        android:id="@+id/imageViewCustomerCartShoopingCart"
        android:layout_width="71dp"
        android:layout_height="39dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.69"
        app:layout_constraintStart_toEndOf="@+id/imageViewCustomerCartLogo"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/customer_cart" />

    <TextView
        android:id="@+id/textViewCustomerCartPastOrders"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="86dp"
        android:text="Past Orders"
        android:textAlignment="textStart"
        android:textColor="@color/black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.079"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageViewPastOrdersCustomerOrder1"
        android:layout_width="361dp"
        android:layout_height="90dp"
        android:layout_marginTop="126dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/rectangle_store" />
    <ImageView
        android:id="@+id/imageViewPastOrdersCustomerStorePhoto"
        android:layout_width="49dp"
        android:layout_height="49dp"
        android:layout_marginBottom="10dp"
        android:layout_marginEnd="302dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintTop_toTopOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintVertical_bias="0.6"
        app:srcCompat="@drawable/store_photo" />

    <TextView
        android:id="@+id/textViewPastOrdersCustomerStoreName"
        android:layout_width="260dp"
        android:layout_height="19dp"
        android:layout_marginBottom="40dp"
        android:layout_marginStart="69dp"
        android:layout_marginTop="10dp"
        android:text="Timmy's Ol' Reliable"
        android:textAlignment="textStart"
        android:textColor="@color/design_default_color_on_secondary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintStart_toStartOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintTop_toTopOf="@+id/imageViewPastOrdersCustomerOrder1" />

    <TextView
        android:id="@+id/textViewPastOrdersCustomerOrderStatus"
        android:layout_width="71dp"
        android:layout_height="19dp"
        android:layout_marginBottom="40dp"
        android:layout_marginEnd="5dp"
        android:layout_marginStart="285dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rectangle"
        android:text="PROCESSING"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="11sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintHorizontal_bias="0.944"
        app:layout_constraintStart_toStartOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintTop_toTopOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintVertical_bias="0.2" />

    <TextView
        android:id="@+id/textViewPastOrdersCustomersOrderDatePrice"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:layout_marginEnd="119dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="35dp"
        android:text="Friday, Nov 20 . CA$20"
        app:layout_constraintBottom_toBottomOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintEnd_toEndOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintHorizontal_bias="0.386"
        app:layout_constraintStart_toStartOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintTop_toTopOf="@+id/imageViewPastOrdersCustomerOrder1"
        app:layout_constraintVertical_bias="0.8" />

</androidx.constraintlayout.widget.ConstraintLayout>